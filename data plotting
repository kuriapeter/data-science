import matplotlib.pyplot as pinchez #creating static, animated, and interactive visualizations
x = [1, 2, 3, 4, 5, 6, 7, 8, 9]
y1 = [1, 3, 5, 3, 1, 3, 5, 3, 1]
y2 = [2, 4, 6, 4, 2, 4, 6, 4, 2]
pinchez.plot(x, y1, label="SEM1")
pinchez.plot(x, y2, label="SEM2")
pinchez.plot()
pinchez.xlabel("X axis")
pinchez.ylabel("Y axis")
pinchez.title("DISTRIBUTION PER SEMESTER")
pinchez.legend() #used to describe elements for a particular area of a graph
